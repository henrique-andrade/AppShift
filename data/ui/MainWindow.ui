<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <object class="GtkWindow" id="window">
    <child>
      <object class="GtkBox" id="box">
        <property name="orientation">vertical</property>
        <child>
          <object class="GtkLabel" id="welcome_label">
           <property name="label">AppShift</property>
           <style>
             <class name="h1"/>
           </style>
              </object>
        </child>
        <child>
          <object class="GtkLabel">
           <property name="label">Migre seus pacotes de um computador para outro.</property>
           <style>
             <class name="h2"/>
           </style>
              </object>
        </child>
        <child>
          <object class="GtkToggleButton" id="list_apps_button">
            <property name="label">Listar apps instalados</property>
            <style>
              <class name="text-button toggle"/>
            </style>
          </object>
        </child>
        <child>
          <object class="GtkToggleButton" id="export_csv_button">
            <property name="label">Exportar para CSV</property>
            <style>
              <class name="text-button toggle"/>
            </style>
          </object>
        </child>
        <child>
          <object class="GtkScrolledWindow" id="scrolled_window">
            <property name="vexpand">True</property>
            <child>
              <object class="GtkTextView" id="output_textview"/>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
